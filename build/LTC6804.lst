   3:Src/LTC6804.c **** //This function writes the config registers of the LTC6804-2
   4:Src/LTC6804.c **** void write_config_6804_2(void)
   5:Src/LTC6804.c **** {
   6:Src/LTC6804.c **** 	//Holds the formatted config data
   7:Src/LTC6804.c **** 	uint8_t data[6] = {0};
   8:Src/LTC6804.c **** 
   9:Src/LTC6804.c **** 	//Holds the command
  10:Src/LTC6804.c **** 	uint16_t command = 0;
  11:Src/LTC6804.c **** 
  12:Src/LTC6804.c **** 	//Format the command
  13:Src/LTC6804.c **** 	command = LTC6804_2_ADDRESS_MODE<<15 | LTC6804_2_ADDRESS<<11 | WRCFG;
  14:Src/LTC6804.c **** 
  15:Src/LTC6804.c **** 	//Populate the data
  16:Src/LTC6804.c **** 	data[0] = config_6804_2_buffer.GPIO5<<3 | config_6804_2_buffer.REFON<<2 | config_6804_2_buffer.SWT
  17:Src/LTC6804.c **** 	data[1] = config_6804_2_buffer.VUVLB;
  18:Src/LTC6804.c **** 	data[2] = config_6804_2_buffer.VOVLB<<4|config_6804_2_buffer.VUVUB;
  19:Src/LTC6804.c **** 	data[3] = config_6804_2_buffer.VOVUB;
  20:Src/LTC6804.c **** 	data[4] = config_6804_2_buffer.DCCLB;
  21:Src/LTC6804.c **** 	data[5] = config_6804_2_buffer.DCTO<<4|config_6804_2_buffer.DCCUB;
  22:Src/LTC6804.c **** 
  23:Src/LTC6804.c **** 	//Call the spi_write_word function
  24:Src/LTC6804.c **** 	SPI_transmit_word(command, data);
  25:Src/LTC6804.c **** 
  26:Src/LTC6804.c **** 	return;
  27:Src/LTC6804.c **** }
